{
  "name": "sentry-yt",
  "version": "0.0.0",
  "description": "Sentry YouTrack integration",
  "license": "AGPL-3.0-only",
  "author": "IT-Hock <support@it-hock.de>",
  "main": "dist/app.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IT-Hock/sentry-yt.git"
  },
  "bugs": {
    "url": "https://github.com/IT-Hock/sentry-yt/issues"
  },
  "homepage": "https://github.com/IT-Hock/sentry-yt#readme",
  "keywords": [
    "sentry",
    "youtrack",
    "integration",
    "webhook",
    "issue-tracking"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "start": "npm run clean && tsc-watch --build tsconfig.build.json --onSuccess \"node dist/app.js\"",
    "build": "npm run clean && tsc --build tsconfig.build.json",
    "lint": "eslint .",
    "test": "jest",
    "test:watch": "jest --watch",
    "docker:build": "npm run build && docker build -t sentry-yt .",
    "docker:run": "docker run -p 3000:3000 --env-file .env sentry-yt",
    "docker:compose:up": "npm run build && docker-compose up --build -d"
  },
  "dependencies": {
    "axios": "^1.9.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~4.4.0",
    "express": "~5.1.0",
    "helmet": "^8.1.0",
    "morgan": "~1.10.1",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "youtrack-rest-client": "^1.6.0"
  },
  "devDependencies": {
    "@eslint/config-helpers": "^0.2.2",
    "@eslint/js": "^9.27.0",
    "@types/cors": "^2.8.18",
    "@types/express": "^5.0.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.19",
    "@types/supertest": "^6.0.3",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/validator": "^13.15.0",
    "@typescript-eslint/eslint-plugin": "~8.32.1",
    "@typescript-eslint/parser": "^8.32.0",
    "dotenv": "^16.5.0",
    "eslint": "^9.27.0",
    "eslint-import-resolver-typescript": "^4.3.4",
    "globals": "^16.1.0",
    "husky": "^9.1.7",
    "jest": "^29.5.14",
    "mocha": "^11.2.2",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "supertest": "^7.1.0",
    "ts-jest": "^29.3.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsc-watch": "^6.3.1",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.0"
  }
}
